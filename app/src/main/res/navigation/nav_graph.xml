<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.hackfrostletsgo.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment2"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_lessons_Scroll"
            app:destination="@id/lessons_Scroll" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.hackfrostletsgo.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">
        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment2"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/lessons_Scroll"
        android:name="com.example.hackfrostletsgo.Lessons_Scroll"
        android:label="fragment_lessons__scroll"
        tools:layout="@layout/fragment_lessons__scroll" />
</navigation>